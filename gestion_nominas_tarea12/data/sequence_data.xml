<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_tarea12_nomina" model="ir.sequence">
            <field name="name">Secuencia Nómina</field>
            <field name="code">tarea12.nomina</field>
            <field name="prefix">NOM-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_tarea12_declaracion" model="ir.sequence">
            <field name="name">Secuencia Declaración Renta</field>
            <field name="code">tarea12.declaracion.renta</field>
            <field name="prefix">RENTA-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
